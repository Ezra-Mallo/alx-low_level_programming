SRC = main.c school.c
CC = gcc
OBJ = $(SRC:.c=.o)
NAME = school
RM: rm $(SRC) $(OBJ) $(NAME)
variables: CC, SRC, OBJ, NAME, RM

all: $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

.PHONY : all, clean all, oclean, fclean, re
clean: 
	rm * swp $(NAME)

oclean :
	rm $(OBJ)

fclean:
	rm $(SRC) $(OBJ) $(NAME)

re:
	RM
